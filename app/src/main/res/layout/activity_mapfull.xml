<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_mapfull"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="sigit.jadwal.view.MapfullActivity">
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbarMapFull"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:fitsSystemWindows="true"
        android:minHeight="?attr/actionBarSize"
        app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        android:background="?attr/colorPrimary">
    </android.support.v7.widget.Toolbar>
    <fragment
        android:layout_below="@+id/toolbarMapFull"
        android:id="@+id/map_full"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="sigit.jadwal.DetiltravelActivity">
    </fragment>
</RelativeLayout>
